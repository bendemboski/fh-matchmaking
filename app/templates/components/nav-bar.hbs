<div class="container">
  {{#if isHost}}
  <a class="navbar-brand" href="#">{{#link-to 'homepage-host'}}<img src="/assets/images/theblockproject_logo_80px.png" alt="the-block-project-logo" height="65px" width="65px">{{/link-to}}</a>
{{else if isAdmin}}
<a class="navbar-brand" href="#">{{#link-to 'homepage-admin'}}<img src="/assets/images/theblockproject_logo_80px.png" alt="the-block-project-logo" height="65px" width="65px">{{/link-to}}</a>
{{else}}
<a class="navbar-brand" href="#">{{#link-to 'resident-list'}}<img src="/assets/images/theblockproject_logo_80px.png" alt="the-block-project-logo" height="65px" width="65px">{{/link-to}}</a>
{{/if}}
  <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
    MENU
    <i class="fa fa-bars" ></i>
  </button>
  <div class="collapse navbar-collapse" id="navbarResponsive">
    <ul class="navbar-nav ml-auto">
      {{yield}}
    </ul>
  </div>
</div>
